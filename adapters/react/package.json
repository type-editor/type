{
    "name": "@type-editor/adapter-react",
    "description": "Type editor React adapter",
    "version": "0.0.1",
    "license": "MIT",
    "homepage": "https://type-editor.io",
    "maintainers": [
        {
            "name": "Type Editor",
            "email": "type-editor@mailbox.org",
            "web": "https://type-editor.io"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/type-editor/type.git"
    },
    "type": "module",
    "sideEffects": false,
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "keywords": [
        "type",
        "type-editor",
        "richtext-editor",
        "prosemirror"
    ],
    "peerDependencies": {
        "@type-editor/adapter-core": "workspace:^",
        "@type-editor/commons": "workspace:^",
        "@type-editor/decoration": "workspace:^",
        "@type-editor/dom-util": "workspace:^",
        "@type-editor/model": "workspace:^",
        "@type-editor/transform": "workspace:^",
        "nanoid": "^5.1.6",
        "react": "*",
        "react-dom": "*"
    },
    "peerDevDependencies": {
        "@type-editor/editor-types": "workspace:^"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "devDependencies": {
        "@type-editor/adapter-core": "workspace:^",
        "@type-editor/commons": "workspace:^",
        "@type-editor/decoration": "workspace:^",
        "@type-editor/dom-util": "workspace:^",
        "@type-editor/editor-types": "workspace:^",
        "@type-editor/model": "workspace:^",
        "@type-editor/transform": "workspace:^",
        "@type-editor/test-builder": "workspace:^",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "nanoid": "^5.1.6",
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    },
    "scripts": {
        "build": "vite build",
        "build:prod": "NODE_ENV=production vite build --mode production",
        "build:dev": "NODE_ENV=development tsc && vite build --mode development",
        "build:ts": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest"
    }
}
