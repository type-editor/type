{
	"name": "@type-editor-compat/keymap",
	"description": "Compatibility layer for @type-editor/keymap providing ProseMirror-compatible type signatures",
	"version": "0.0.1",
	"license": "MIT",
	"homepage": "https://type-editor.io",
	"type": "module",
	"sideEffects": false,
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"dependencies": {
		"@type-editor-compat/state": "workspace:^",
		"@type-editor/keymap": "workspace:^"
	},
	"scripts": {
		"build": "vite build",
		"build:prod": "NODE_ENV=production vite build --mode production",
		"build:dev": "NODE_ENV=development tsc && vite build --mode development",
		"build:ts": "tsc --noEmit"
	}
}
