{
    "extends": "../../../tsconfig.base.json",
    "compilerOptions": {
        "strict": false,
        "strictNullChecks": false,
        "noUncheckedIndexedAccess": false,
        "forceConsistentCasingInFileNames": false,
        "declarationMap": true,
        "sourceMap": true,
        "paths": {
            "@src/*": ["../src/*"]
        }
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "../node_modules",
        "dist"
    ]
}
